        </eTypeArguments>
        <eClassifier xsi:type="ecore:EDataType" href="http://www.eclipse.org/emf/2002/Ecore#//EMap"/>
      </eGenericType>
    </eOperations>
    <eOperations name="countDocumentsAfterDate">
